{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Playlist Preview",
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "images": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string"
          }
        }
      }
    },
    "tracks": {
      "type": "object",
      "properties": {
        "href": {
          "type": "string"
        },
        "total": {
          "type": "number"
        }
      }
    }
  }
}
